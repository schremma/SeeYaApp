<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.group16.seeyaapp"
    android:versionCode="1"
    android:versionName="0.5">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/seeyalogo_192_icon"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.NoActionBar">
        <activity android:name=".main.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <service
            android:name=".communication.CommunicationService"
            android:theme="@style/Theme.AppCompat.NoActionBar" />

        <activity
            android:name=".login.LoginActivity"
            android:theme="@style/Theme.AppCompat.NoActionBar" />
        <activity
            android:name=".register.RegisterActivity"
            android:theme="@style/Theme.AppCompat.NoActionBar" />
        <activity
            android:name=".activity.categories.CategoriesActivity"
            android:theme="@style/Theme.AppCompat.NoActionBar" />
        <!--<activity android:name=".main.MainActivity" />-->
        <activity
            android:name=".activity.details.EditableActivity"
            android:theme="@style/Theme.AppCompat.NoActionBar" />
        <activity
            android:name=".activity.details.DetailActivity"
            android:theme="@style/Theme.AppCompat.NoActionBar" />
        <activity
            android:name=".activity.list.TestListActivity"
            android:theme="@style/Theme.AppCompat.NoActionBar" />
        <activity
            android:name=".navigation.CreatePage"
            android:theme="@style/Theme.AppCompat.NoActionBar" />

        <activity android:name=".activity.list.mainlist.MainListActivity" />
        <activity android:name=".activity.list.HeadlineListActivity" />
         <activity android:name=".navigation.DemoPage"></activity>

        <activity android:name=".activity.details.PublishableActivity"></activity>
    </application>

</manifest>
